
{% extends "base.jinja2" %}

{% block title %} {{ folder_obj.full }} {% endblock %}

{% block self %}
<h1>{{folder_obj.full}}</h1>
<h2>Folder number {{folder_obj.number}}</h2>
{% endblock %}

{% block table %}
<table class="table">
  <thead class="thead-dark">
    <tr>
      <th scope="col">#</th>
      <th scope="col"; align="left">Document Name</th>

    </tr>
  </thead>
  <tbody>
  {% for item in document_objs %}
    <tr>
      <th scope="row">{{ loop.index }} </th>

      <td><a href = "/dj_comp_hist/doc/{{item.pk}}">{{ item }}</a></td>
    </tr>
  {% endfor %}
  </tbody>
</table>

{% endblock %}
