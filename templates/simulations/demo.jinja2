{% extends "simulations/base.jinja2" %}

{% block buttons %}
    <button id="step_button" data-step="11" data-intro="Press this button to execute the next instruction
    .">Step</button>
    <button id="run_button" data-step="12" data-intro="Press this button to run the program.">Run</button>
    <button id="reset_button" data-step="13" data-intro="Press this button to reset the computer to its initial state
    .">Reset</button>
{% endblock %}