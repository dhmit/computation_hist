{% extends "base.jinja2" %}



{% block title %}
    Home Page
{% endblock %}

{% block header %}
    <h1 class="typing">Welcome to the Archives of the MIT Computation Center</h1>
{% endblock %}

{% block content %}
<div class="container">
    <div class="row">
        {% for story in stories %}
            <div class='col-md-4'>
                {% include 'archives/stories/' + story + '-teaser.jinja2' %}
            </div>
        {% endfor %}
    </div>
</div>
{% endblock %}
